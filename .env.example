# FeedPrism Production Environment Variables
# Copy this to .env and fill in your values

# OpenAI API Key (required for LLM extraction)
OPENAI_API_KEY=sk-your-openai-api-key

# Qdrant Configuration (defaults work with docker-compose)
QDRANT_HOST=qdrant
QDRANT_PORT=6333

# Demo Mode (set to true for hackathon demo)
DEMO_MODE=true
